package javademo;

public class Static {
	static int a=10;
	int b=20;
	
	static void m1()
	{
		System.out.println("Hii all");
	}
	
	void m2()
	{
		System.out.println("How are you?");
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println(a);
		Static st = new Static();
		System.out.println(st.b);
		
		m1();
		st.m2();

	}

}
